{
  "AllowedHosts": "*",
  "AzureAdB2C": {
    "Instance": "https://holefeeder.b2clogin.com",
    "Domain": "holefeeder.onmicrosoft.com",
    "ClientId": "3cb767b7-c113-4080-87e9-83519f09dcb2",
    "SignedOutCallbackPath": "/signout/B2C_1_Signup_Signin",
    "SignUpSignInPolicyId": "B2C_1_Signup_Signin",
    "SwaggerClientId": "cf9c4a80-2568-4c8e-acf3-0ba6e9937fbf"
  },
  "Cors": {
    "AllowedOrigins": ["https://holefeeder.drifterapps.app"]
  },
  "Serilog": {
    "Properties": {
      "ApplicationName": "DrifterApps.Holefeeder.Api",
      "ApplicationVersion": "1.0.0"
    },
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Seq"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": {
          "maximumDestructuringDepth": 4
        }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": {
          "maximumStringLength": 100
        }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": {
          "maximumCollectionCount": 10
        }
      }
    ]
  }
}
